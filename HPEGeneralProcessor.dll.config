<Configuration xmlns="http://www.bentley.com/OrchestrationFramework/ExtConfigSchema.xsd">
    <SupportingPath Directory="[SOFTWARE\Bentley\AutomationServices\Server:InstallDir]"/>
    <SupportingPath Directory="[SOFTWARE\Bentley\AutomationServices\Administrator:InstallDir]"/>
    <EntryAssembly>
        <Name>orchadmin</Name>
        <RunSpecifiedLoaders>
            <ExtensionLoader Type="HPE.Automation.Extensions.HPEGeneralProcessor.JobDefinitionExtLoader"/>
        </RunSpecifiedLoaders>
  </EntryAssembly>
  <EntryAssembly>
    <Name>pasSmartDispatcher</Name>
        <RunSpecifiedLoaders>
            <ExtensionLoader Type="HPE.Automation.Extensions.HPEGeneralProcessor.SmartDispatcherExtLoader"/>
        </RunSpecifiedLoaders>
  </EntryAssembly>
  <EntryAssembly>
    <Name>pasDelegateProcessor</Name>
        <RunSpecifiedLoaders>
            <ExtensionLoader Type="HPE.Automation.Extensions.HPEGeneralProcessor.DelegateExtLoader"/>
        </RunSpecifiedLoaders>
  </EntryAssembly>
  <EntryAssembly>
    <Name>ustation</Name>
        <RunAllLoaders/>
  </EntryAssembly>
</Configuration>


